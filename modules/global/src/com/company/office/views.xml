<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<views xmlns="http://schemas.haulmont.com/cuba/view.xsd">
    <view class="com.company.office.entity.Step"
          extends="_local"
          name="step-view">
        <property name="actions"
                  view="_minimal">
            <property name="type"/>
            <property name="description"/>
            <property name="template"
                      view="_minimal">
                <property name="size"/>
            </property>
            <property name="workDays"/>
        </property>
        <property name="createTs"/>
        <property name="createdBy"/>
        <property name="updateTs"/>
        <property name="updatedBy"/>
        <property name="users"
                  view="_local"/>
    </view>
    <view class="com.company.office.entity.StepAction"
          extends="_local"
          name="stepAction-view">
        <property name="template"
                  view="_minimal">
            <property name="size"/>
        </property>
        <property name="createTs"/>
        <property name="createdBy"/>
        <property name="updateTs"/>
        <property name="updatedBy"/>
    </view>
    <view class="com.company.office.entity.RequestAction"
          extends="_local"
          name="requestAction-view">
        <property name="template"
                  view="_minimal">
            <property name="size"/>
        </property>
        <property name="file"
                  view="_minimal">
            <property name="size"/>
        </property>
        <property name="createTs"/>
        <property name="createdBy"/>
        <property name="updateTs"/>
        <property name="updatedBy"/>
    </view>
    <view class="com.company.office.entity.RequestCommunication"
          extends="_local"
          name="requestCommunication-view">
        <property name="file"
                  view="_minimal">
            <property name="size"/>
        </property>
        <property name="createTs"/>
        <property name="createdBy"/>
        <property name="updateTs"/>
        <property name="updatedBy"/>
    </view>
    <view class="com.company.office.entity.Request"
          extends="_local"
          name="request-view">
        <property name="step"
                  view="_minimal">
        </property>
        <property name="communications"
                  view="requestCommunication-view"/>
        <property name="user"
                  view="_minimal"/>
        <property name="applicant"
                  view="_minimal">
        </property>
        <property name="actions"
                  view="requestAction-view"/>
        <property name="steps"
                  view="requestStep-view">
        </property>
        <property name="createTs"/>
        <property name="createdBy"/>
        <property name="updateTs"/>
        <property name="updatedBy"/>
    </view>
    <view class="com.company.office.entity.StepUser"
          extends="_local"
          name="stepUser-view">
        <property name="user"
                  view="_minimal"/>
        <property name="createTs"/>
        <property name="createdBy"/>
        <property name="updateTs"/>
        <property name="updatedBy"/>
    </view>
    <view class="com.company.office.entity.RequestStep"
          extends="_local"
          name="requestStep-view">
        <property name="step"
                  view="_minimal"/>
        <property name="user"
                  view="_minimal"/>
        <property name="updateTs"/>
        <property name="updatedBy"/>
        <property name="createTs"/>
        <property name="createdBy"/>
    </view>
</views>
