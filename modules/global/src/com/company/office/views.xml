<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<views xmlns="http://schemas.haulmont.com/cuba/view.xsd">
    <view class="com.company.office.entity.Position"
          extends="_local"
          name="position-view">
        <property name="actions"
                  view="positionAction-view">
        </property>
        <property name="createTs"/>
        <property name="createdBy"/>
        <property name="updateTs"/>
        <property name="updatedBy"/>
        <property name="users"
                  view="positionUser-view"/>
    </view>
    <view class="com.company.office.entity.PositionAction"
          extends="_local"
          name="positionAction-view">
        <property name="template"
                  view="_minimal">
            <property name="size"/>
        </property>
        <property name="createTs"/>
        <property name="createdBy"/>
        <property name="updateTs"/>
        <property name="updatedBy"/>
    </view>
    <view class="com.company.office.entity.RequestStepAction"
          extends="_local"
          name="requestStepAction-view">
        <property name="template"
                  view="_minimal">
            <property name="size"/>
        </property>
        <property name="file"
                  view="_minimal">
            <property name="size"/>
        </property>
        <property name="createTs"/>
        <property name="createdBy"/>
        <property name="updateTs"/>
        <property name="updatedBy"/>
    </view>
    <view class="com.company.office.entity.RequestStepCommunication"
          extends="_local"
          name="requestStepCommunication-view">
        <property name="createTs"/>
        <property name="createdBy"/>
        <property name="updateTs"/>
        <property name="updatedBy"/>
        <property name="questionFile"
                  view="_minimal">
            <property name="size"/>
        </property>
        <property name="answerFile"
                  view="_minimal"/>
    </view>
    <view class="com.company.office.entity.Request"
          extends="_local"
          name="request-view">
        <property name="step"
                  view="_minimal">
            <property name="state"/>
            <property name="user"
                      view="_minimal"/>
            <property name="description"/>
            <property name="submitted"/>
            <property name="approved"/>
            <property name="penalty"/>
            <property name="updateTs"/>
            <property name="updatedBy"/>
            <property name="createTs"/>
            <property name="createdBy"/>
            <property name="submissionTerm"/>
            <property name="approvalTerm"/>
            <property name="position"
                      view="_minimal"/>
        </property>
        <property name="applicant"
                  view="_minimal">
            <property name="email"/>
        </property>
        <property name="steps"
                  view="requestStep-view">
            <property name="actions"
                      view="_minimal">
                <property name="type"/>
                <property name="description"/>
                <property name="template"
                          view="_minimal">
                    <property name="size"/>
                </property>
                <property name="file"
                          view="_minimal">
                    <property name="size"/>
                </property>
                <property name="message"/>
                <property name="submitted"/>
                <property name="approved"/>
                <property name="createTs"/>
                <property name="createdBy"/>
                <property name="updateTs"/>
                <property name="updatedBy"/>
            </property>
        </property>
        <property name="createTs"/>
        <property name="createdBy"/>
        <property name="updateTs"/>
        <property name="updatedBy"/>
        <property name="logs"
                  view="requestLog-view">
        </property>
    </view>
    <view class="com.company.office.entity.PositionUser"
          extends="_local"
          name="positionUser-view">
        <property name="user"
                  view="_minimal"/>
        <property name="createTs"/>
        <property name="createdBy"/>
        <property name="updateTs"/>
        <property name="updatedBy"/>
    </view>
    <view class="com.company.office.entity.RequestStep"
          extends="_local"
          name="requestStep-view">
        <property name="user"
                  view="_minimal"/>
        <property name="updateTs"/>
        <property name="updatedBy"/>
        <property name="createTs"/>
        <property name="createdBy"/>
        <property name="position"
                  view="_minimal"/>
    </view>
    <view class="com.company.office.entity.RequestLog"
          extends="_local"
          name="requestLog-view">
        <property name="createTs"/>
        <property name="createdBy"/>
        <property name="updateTs"/>
        <property name="updatedBy"/>
    </view>
</views>
